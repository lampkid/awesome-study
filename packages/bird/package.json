{
  "name": "bird",
  "version": "0.0.1",
  "description": "your own project",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "githooks": "git config core.hooksPath .githooks",
    "premock": "npm run githooks",
    "predev": "npm run githooks",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.js --progress",
    "mock": "cross-env NODE_ENV=development MOCK=true webpack-dev-server --config webpack/webpack.config.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --output-public-path //mystatic.com/",
    "pre": "cross-env NODE_ENV=pre webpack --config webpack/webpack.config.js --output-public-path //mystatic.com/pre/",
    "commit": "git cz",
    "changelog-all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "doc": "typedoc --ignoreCompilerErrors --out docs src"
  },
  "keywords": [
    "webpack",
    "ts",
    "react"
  ],
  "author": "lampkid",
  "license": "GPL-3.0",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
