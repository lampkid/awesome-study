{
  "name": "midway",
  "version": "0.0.1",
  "author": "lampkid",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=local && nodemon bin/www.js",
    "dev": "pm2 start pm2.json --env development --name midway-server-dev",
    "test": "pm2 start pm2.json --env test --name midway-server-test",
    "sandbox": "pm2 start pm2.json --env sandbox --name midway-server-sandbox",
    "online_pre": "pm2 start pm2.json --env online_pre --name midway-server-online_pre",
    "online": "pm2 start pm2.json --env production --name midway-server-online",
    "stop": "pm2 stop all",
    "eslint:fix": "node_modules/.bin/eslint *.js --fix"
  },
  "dependencies": {
    "@koa/cors": "2",
    "ajv": "^6.6.1",
    "ajv-errors": "^1.0.0",
    "ajv-i18n": "^3.3.0",
    "ali-oss": "^6.1.1",
    "async-validator": "^1.10.0",
    "chance": "^1.0.18",
    "csv": "^5.1.1",
    "debug": "^2.6.3",
    "ejs": "^2.6.2",
    "faker": "^4.1.0",
    "get-installed-path": "^4.0.8",
    "html-minifier": "^4.0.0",
    "iconv-lite": "^0.4.24",
    "json-schema-faker": "^0.5.0-rc16",
    "koa": "^2.2.0",
    "koa-body": "^4.0.4",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-filestore": "^1.0.4",
    "koa-i18n": "^2.1.0",
    "koa-json": "^2.0.2",
    "koa-locale": "^1.3.0",
    "koa-logger": "^2.0.1",
    "koa-mount": "^4.0.0",
    "koa-onerror": "^1.2.1",
    "koa-proxy": "^1.0.0-alpha.2",
    "koa-router": "^7.1.1",
    "koa-session": "^5.12.2",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "lodash": "^4.17.10",
    "mysql2": "^1.5.3",
    "node-ral": "^0.18.1",
    "path-to-regexp": "^2.2.1",
    "pm2": "^3.2.4",
    "query-string": "^6.1.0",
    "scp2": "^0.5.0",
    "sequelize": "^5.0.0-beta.15",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "nodemon": "^1.8.1"
  }
}
